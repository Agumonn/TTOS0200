        <TextBlock x:Name="korkeusblock" HorizontalAlignment="Left" Height="24" Margin="327,55,0,0" TextWrapping="Wrap" Text="Ikkunan korkeus" VerticalAlignment="Top" Width="109"/>
        <TextBlock x:Name="karmiblock" HorizontalAlignment="Left" Height="24" Margin="327,87,0,0" TextWrapping="Wrap" Text="Karmipuun leveys" VerticalAlignment="Top" Width="109"/>
        <TextBox x:Name="ikkunapabox" HorizontalAlignment="Left" Height="24" Margin="430,197,0,0" TextWrapping="Wrap" Text="2500" VerticalAlignment="Top" Width="84" SpellCheck.IsEnabled="True"/>
        <TextBox x:Name="lasinpabox" HorizontalAlignment="Left" Height="24" Margin="430,230,0,0" TextWrapping="Wrap" Text="1000" VerticalAlignment="Top" Width="84"/>
        <TextBox x:Name="karmipiiribox" HorizontalAlignment="Left" Height="24" Margin="430,263,0,0" TextWrapping="Wrap" Text="45" VerticalAlignment="Top" Width="84"/>
        <TextBlock x:Name="ikkunapablock" HorizontalAlignment="Left" Height="24" Margin="327,197,0,0" TextWrapping="Wrap" Text="Ikkunan pinta-ala" VerticalAlignment="Top" Width="98"/>
        <TextBlock x:Name="lasinpablock" HorizontalAlignment="Left" Height="24" Margin="327,230,0,0" TextWrapping="Wrap" Text="Lasin pinta-ala" VerticalAlignment="Top" Width="98"/>
        <TextBlock x:Name="karmipiiriblock" HorizontalAlignment="Left" Height="24" Margin="327,263,0,0" TextWrapping="Wrap" Text="Karmin piiri" VerticalAlignment="Top" Width="98"/>
        <TextBox x:Name="tulosbox" HorizontalAlignment="Left" Height="21" Margin="327,292,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="98"/>

